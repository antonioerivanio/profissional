	<queryString>
		<![CDATA[SELECT
    *
FROM
    (
        SELECT
            nome,
            CASE
                WHEN id_pessoal IN (
                    SELECT
                        idpessoal
                    FROM
                        srh.tb_funcional
                    WHERE
                            dataexercicio = '21/08/2017'
                        AND idtipomovimentoentrada IN ( 3, 43 )
                ) THEN
                    'TCM'
                ELSE
                    'TCE'
            END origem,
            ano_referencia,
            dias_fruicao
        FROM
            (
                SELECT
                    p.id                   id_pessoal,
                    p.nomecompleto         nome,
                    fe.anoreferencia       ano_referencia,
                    SUM(fe.qtdedias)       dias_fruicao
                FROM
                         srh.tb_ferias fe
                    INNER JOIN srh.tb_tipoferias    tfe ON fe.tipoferias = tfe.id
                    INNER JOIN srh.tb_funcional     fu ON fe.idfuncional = fu.id
                    INNER JOIN srh.tb_pessoal       p ON fu.idpessoal = p.id
                    INNER JOIN srh.tb_ocupacao      o ON fu.idocupacao = o.id
                WHERE
                    fe.tipoferias IN ( 1, 3 )                    
                    AND fu.datasaida IS NULL
                    AND fu.status <> 5
                    AND o.tipoocupacao <> 1
                    AND o.situacao < 3
                    $P!{FILTRO_FERIAS}                    
                GROUP BY
                    p.id,
                    p.nomecompleto,
                    fe.anoreferencia,
                    fe.periodo
            )
        WHERE
            dias_fruicao < 30
        ORDER BY
            nome,
            ano_referencia
    )
WHERE 1=1
     $P!{FILTRO_ORIGEM}]]>
	</queryString>
