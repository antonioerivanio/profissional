	<queryString>
		<![CDATA[select
	fe.id,
	tf.DESCRICAO,
	fe.ANOREFERENCIA,
	fe.PERIODO,
	fe.INICIO,
	fe.FIM,
	fe.QTDEDIAS DIAS

from
	TB_FERIAS fe
	inner join TB_TIPOFERIAS tf on fe.TIPOFERIAS = tf.ID
	inner join TB_FUNCIONAL f on fe.IDFUNCIONAL = f.ID

where
	f.idpessoal = $P!{IDPESSOAL}

order by fe.ANOREFERENCIA desc, fe.INICIO desc]]>
	</queryString>
