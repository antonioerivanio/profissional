	<queryString>
		<![CDATA[SELECT DISTINCT rownum idnum,
	TB_PESSOAL.NOMECOMPLETO nomeCompleto,
	TB_FERIAS.ANOREFERENCIA anoReferencia,
	TB_FERIAS.INICIO inicio,
	TB_FERIAS.FIM fim,
	tb_tipoferias.DESCRICAO tipoFerias
	FROM SRH.TB_FERIAS
	INNER JOIN SRH.TB_FUNCIONAL ON TB_FERIAS.IDFUNCIONAL   = TB_FUNCIONAL.ID
	INNER JOIN SRH.TB_PESSOAL ON TB_FUNCIONAL.IDPESSOAL  = TB_PESSOAL.ID
	INNER JOIN SRH.tb_tipoferias ON tb_tipoferias.ID = TB_FERIAS.TIPOFERIAS
	WHERE 1=1 and
( TB_FUNCIONAL.IDSETOR = $P!{IDSETORFILTRO} OR $P!{IDSETORFILTRO} is null) AND
( TB_FERIAS.TIPOFERIAS in ( $P!{TIPOSFERIASFILTRO} ) OR '$P!{TIPOSFERIASFILTRO}' is null) AND
( TB_FERIAS.INICIO >= to_date( '$P!{INICIOFILTRO}' , 'dd/MM/yyyy' )  OR $P!{INICIOFILTRO} is null  ) AND
( TB_FERIAS.INICIO <= to_date( '$P!{FIMFILTRO}' , 'dd/MM/yyyy' ) OR $P!{FIMFILTRO} is null )
ORDER BY TB_PESSOAL.NOMECOMPLETO]]>
	</queryString>
