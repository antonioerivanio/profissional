	<queryString>
		<![CDATA[select
	l.inicio,
	l.fim,
	(le.qtdedias * 2) ass,
	l.obs

from
	tb_licenca l
	inner join tb_direitolicencaesp le on l.idlicencaesp = le.id
	inner join tb_pessoal p on l.idpessoal = p.id

where
	l.excluitemposerv = 0
	and le.contaremdobro > 0
	and p.id = $P{IDPESSOAL}

order by l.inicio desc]]>
	</queryString>
