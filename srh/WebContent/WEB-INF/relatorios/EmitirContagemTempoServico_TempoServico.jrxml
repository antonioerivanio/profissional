	<parameter name="SAIDA" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
	DIFERENCA_DATA( nvl(f.datasaida, nvl($P{SAIDA}, SYSDATE)), f.dataexercicio) AS datadif,
        o.nomenclatura,
        f.dataexercicio,
        nvl(f.datasaida, nvl($P{SAIDA}, SYSDATE)) AS datasaida,
        f.descricaonomeacao

FROM
	tb_funcional f
	inner join tb_pessoal p on f.idpessoal = p.id
  	inner join tb_ocupacao o on f.idocupacao = o.id


WHERE
	p.id = $P{IDPESSOAL}


ORDER BY
	f.dataexercicio DESC]]>
	</queryString>
