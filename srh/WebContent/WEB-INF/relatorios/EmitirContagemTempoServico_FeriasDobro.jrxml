	<queryString>
		<![CDATA[select
	fe.anoreferencia,
	(fe.qtdedias + fe.qtdedias) as qtdedias,
	fe.obs,
	f.id

from
	tb_ferias fe,
	tb_funcional f

where
	fe.idfuncional = f.id
	and fe.tipoferias = 5
	and f.idPessoal = $P{IDPESSOAL}

order by f.id]]>
	</queryString>
