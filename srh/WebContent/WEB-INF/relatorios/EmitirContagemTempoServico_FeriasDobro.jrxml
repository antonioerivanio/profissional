	<queryString>
		<![CDATA[select
	fe.anoreferencia,
	(fe.qtdedias * 2) as qtdedias,
	fe.obs,
	f.id

from
	tb_ferias fe
	inner join tb_funcional f on fe.idfuncional = f.id

where
	f.idPessoal = $P{IDPESSOAL}
	and fe.tipoferias = 5

order by f.id]]>
	</queryString>
