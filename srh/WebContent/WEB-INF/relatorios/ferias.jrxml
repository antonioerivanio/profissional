	<queryString>
		<![CDATA[select t.descricao, fe.ANOREFERENCIA, fe.PERIODO, fe.INICIO, fe.FIM, fe.QTDEDIAS, fu.MATRICULA, fu.NOMECOMPLETO, o.nomenclatura
from TB_FERIAS fe
inner join TB_FUNCIONAL fu on fe.idfuncional = fu.id
inner join TB_OCUPACAO o on fu.idocupacao = o.id
inner join tb_tipoferias t on fe.tipoferias = t.id
