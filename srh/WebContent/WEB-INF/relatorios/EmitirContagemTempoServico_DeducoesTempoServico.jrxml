	<queryString>
		<![CDATA[select
	decode(falta, 1, 'Falta', 'Dedução') as TIPO,
	d.data_inicio,
	d.data_fim,
	d.qtdedias,
	d.descricao as OBS,
	p.id
from
	tb_deducao d
	inner join tb_pessoal p on d.idpessoal = p.id
where
	p.id = $P{IDPESSOAL}
	and (falta <> 1 or (falta = 1 and d.data_inicio <= TO_DATE('15/12/1998', 'dd/mm/yyyy')))
union

select
	t.descricao as TIPO,
	l.inicio as DATA_INICIO,
	l.fim as DATA_FIM,
	(l.fim-l.inicio) + 1 as QTDEDIAS,
	l.obs,
	l.id
from
	tb_licenca l
	inner join tb_tipolicenca t on t.id = l.idtipolicenca
where
	l.idPessoal = $P{IDPESSOAL}
	and l.excluitemposerv = 1

order by data_inicio desc]]>
	</queryString>
