	<parameter name="SAIDA" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
	DIFERENCA_DATA( nvl(f.datasaida, nvl($P{SAIDA}, SYSDATE)), f.dataexercicio) AS datadif,
        o.nomenclatura,
        f.dataexercicio,
        nvl(f.datasaida, nvl($P{SAIDA}, SYSDATE)) AS datasaida,
        f.descricaonomeacao

FROM
	tb_funcional f,
	tb_ocupacao o,
	tb_pessoal p

WHERE
	f.idocupacao = o.id
	AND f.idpessoal = p.id
	AND p.id = $P{IDPESSOAL}

ORDER BY f.dataexercicio DESC]]>
	</queryString>
