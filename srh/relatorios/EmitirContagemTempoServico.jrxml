	<queryString>
		<![CDATA[select
	f.ID,
	f.NOMECOMPLETO,
	p.NOMEMAE,
	p.RG,
	p.EMISSORRG,
	u.NOME,
	o.NOMENCLATURA,
	f.DATAEXERCICIO,
	s.NMSETOR

from TB_FUNCIONAL f
	inner join TB_PESSOAL p on f.idpessoal = p.id
	left join TB_UF u on p.uf = u.uf
	inner join TB_OCUPACAO o on f.idocupacao = o.id
	inner join SAPJAVA.setor s on f.idsetor = s.idsetor

where f.ID = $P!{IDFUNCIONAL}

order by f.ID]]>
	</queryString>
