	<queryString>
		<![CDATA[SELECT 	S.NMSETOR nomeSetor,
	F.NOMECOMPLETO nomeCompleto,
	O.NOMENCLATURA cargo,
    decode(CR.REFERENCIA, null, ' ', 'REF' || CR.REFERENCIA) as REFERENCIA,
	RC.NOMENCLATURA representacao,
	RC.SIMBOLO simbolo
FROM SRH.TB_REPRESENTACAOFUNCIONAL RF RIGHT JOIN SRH.TB_FUNCIONAL F ON RF.IDFUNCIONAL = F.ID INNER JOIN SAPJAVA.SETOR S ON F.IDSETOR = S.IDSETOR INNER JOIN SRH.TB_OCUPACAO O ON F.IDOCUPACAO = O.ID LEFT JOIN SRH.TB_REPRESENTACAOCARGO RC ON RF.IDREPRESENTACAOCARGO = RC.ID LEFT JOIN SRH.TB_CLASSEREFERENCIA CR ON F.IDCLASSEREFERENCIA = CR.ID
WHERE F.IDTIPOMOVIMENTOSAIDA Is Null AND F.ATIVOFP=1 AND RF.IDTIPOMOVIMENTOFIM Is Null
ORDER BY S.NRORDEMSETORFOLHA, F.NOMECOMPLETO]]>
