	<queryString>
		<![CDATA[SELECT DISTINCT TB_PESSOAL.NOMECOMPLETO,TB_FUNCIONAL.MATRICULA, TB_CURSOPROFISSIONAL.DESCRICAO CURSO, TB_AREAPROFISSIONAL.DESCRICAO AREA,
TB_CURSOPROFISSIONAL.INICIO,TB_CURSOPROFISSIONAL.FIM ,TB_CURSOPROFISSIONAL.CARGAHORARIA
FROM TB_FUNCIONAL INNER JOIN (((((TB_PESSOAL INNER JOIN TB_PESSOALCURSOPROF ON TB_PESSOAL.ID = TB_PESSOALCURSOPROF.IDPESSOAL)
INNER JOIN TB_CURSOPROFISSIONAL ON TB_PESSOALCURSOPROF.IDCURSOPROFISSIONAL = TB_CURSOPROFISSIONAL.ID)
INNER JOIN TB_AREAPROFISSIONAL ON TB_CURSOPROFISSIONAL.IDAREAPROFISSIONAL = TB_AREAPROFISSIONAL.ID)
INNER JOIN TB_COMPETENCIACURSO ON TB_CURSOPROFISSIONAL.ID = TB_COMPETENCIACURSO.IDCURSOPROFISSIONAL)
INNER JOIN TB_COMPETENCIA ON TB_COMPETENCIACURSO.IDCOMPETENCIA = TB_COMPETENCIA.ID) ON TB_FUNCIONAL.IDPESSOAL = TB_PESSOAL.ID
$P!{FILTRO}
ORDER BY TB_CURSOPROFISSIONAL.INICIO]]>
